<?xml version="1.0" encoding="UTF-8"?>
<report name="sample_report" orientation="Landscape">
  <style name="Arial_Normal" isDefault="true" fontName="Arial"
    fontSize="12" pdfFontName="c:\tahoma.ttf" pdfEncoding="Cp1251"
    isPdfEmbedded="false" />

  <queryString>
    <![CDATA[ select nameInst, nameSpec, namePlan, nameThr from Inst, Spec, Plan, Thr where Inst.idInst = Spec.idInst and Spec.idSpec = Plan.idSpec and Inst.idInst=$P{idInst}; ]]>
  </queryString>

  <field name="nameInst" class="QString" />
  <field name="nameSpec" class="QString" />
  <field name="namePlan" class="QString" />
  <field name="nameThr" class="QString" />

  <title>
    <band height="35">
      <staticText>
        <reportElement x="10" y="10" width="150" height="20" />
        <text><![CDATA[$P{title}]]></text>
      </staticText>
      <rect>
        <reportElement x="0" y="0" width="535" height="35" />
      </rect>
    </band>
  </title>
  <detail>
    <band height="20">
      <line>
        <reportElement x="0" y="0" width="1" height="20" />
      </line>
      <textField>
        <reportElement x="5" y="0" width="180" height="20" />
        <textFieldExpression class="QString"><![CDATA[$F{nameInst}]]></textFieldExpression>
      </textField>
      <line>
        <reportElement x="185" y="0" width="1" height="20" />
      </line>
      <textField>
        <reportElement x="190" y="0" width="200" height="20" />
        <textFieldExpression class="QString"><![CDATA[$F{nameSpec}]]></textFieldExpression>
      </textField>
      <line>
        <reportElement x="390" y="0" width="1" height="20" />
      </line>
      <textField>
        <reportElement x="405" y="0" width="190" height="20" />
        <textFieldExpression class="QString"><![CDATA[$F{namePlan}]]></textFieldExpression>
      </textField>
	<line>
        <reportElement x="490" y="0" width="1" height="20" />
      </line>
      <textField>
        <reportElement x="505" y="0" width="190" height="20" />
        <textFieldExpression class="QString"><![CDATA[$F{nameThr}]]></textFieldExpression>
      </textField>
      <line>
        <reportElement x="534" y="0" width="1" height="20" />
      </line>
      <line>
        <reportElement x="0" y="19" width="535" height="1" />
      </line>
    </band>
  </detail>

</report>
